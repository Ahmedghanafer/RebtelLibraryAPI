{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=RebtelLibraryAPI;Trusted_Connection=true;MultipleActiveResultSets=true"
  },
  "DatabaseSeeding": {
    "EnableSqlSeeding": true,
    "ForceReseed": false,
    "SeedScriptsPath": "Infrastructure/Data/SeedData/SqlSeedScripts",
    "EnableDetailedLogging": true
  }
}
